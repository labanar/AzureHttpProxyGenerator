{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "canadaeast"
    },
    "networkInterfaceName": {
      "value": "proxy3365"
    },
    "networkSecurityGroupName": {
      "value": "[concat('proxy-nsg')]"
    },
    "networkSecurityGroupRules": {
      "value": [
        {
          "name": "SSH",
          "properties": {
            "priority": 300,
            "protocol": "TCP",
            "access": "Allow",
            "direction": "Inbound",
            "sourceAddressPrefix": "*",
            "sourcePortRange": "*",
            "destinationAddressPrefix": "*",
            "destinationPortRange": "22"
          }
        },
        {
          "name": "ProxyPort",
          "properties": {
            "protocol": "*",
            "sourcePortRange": "*",
            "destinationPortRange": "3128",
            "sourceAddressPrefix": "*",
            "destinationAddressPrefix": "*",
            "access": "Allow",
            "priority": 310,
            "direction": "Inbound",
            "sourcePortRanges": [],
            "destinationPortRanges": [],
            "sourceAddressPrefixes": [],
            "destinationAddressPrefixes": []
          }
        }
      ]
    },
    "subnetName": {
      "value": "default"
    },
    "virtualNetworkName": {
      "value": "proxy-vnet"
    },
    "addressPrefixes": {
      "value": [
        "10.0.2.0/24"
      ]
    },
    "subnets": {
      "value": [
        {
          "name": "default",
          "properties": {
            "addressPrefix": "10.0.2.0/24"
          }
        }
      ]
    },
    "publicIpAddressName": {
      "value": "proxy3-ip"
    },
    "publicIpAddressType": {
      "value": "Dynamic"
    },
    "publicIpAddressSku": {
      "value": "Basic"
    },
    "virtualMachineName": {
      "value": "proxy3"
    },
    "virtualMachineComputerName": {
      "value": "proxy"
    },
    "virtualMachineRG": {
      "value": "ProxyGroupA"
    },
    "osDiskType": {
      "value": "Premium_LRS"
    },
    "virtualMachineSize": {
      "value": "Standard_B1ls"
    },
    "adminUsername": {
      "value": "azureuser"
    },
    "adminPassword": {
      "value": null
    }
  }
}